config system
        option hostname 'HanFI'
        option timezone 'UTC'
        option ttylogin '0'
        option log_size '64'
        option urandom_seed '0'

config timeserver 'ntp'
        option enabled '1'
        option enable_server '0'
        list server 'time1.aliyun.com'
        list server 'cn.pool.ntp.org'
        list server 'time1.apple.com'
        list server '120.24.166.46'
